<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="../Front-end/stylesheets/main.css">
    <!-- <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.js"></script> -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="../Front-end/scripts/script.js"></script>
    <title>BucketList</title>
  </head>
  <body>
    <!--NAV BAR-->
    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="/">BucketList</a>
        </div>
        <div>
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/sign-up">Sign Up</a></li>
            <li><a href="/login">Log In</a></li>
          </ul>
        </div>
      </div>
    </nav>
    <!-- NAV BAR END -->
    <!-- CONTAINER -->
    <div class="container">
      <h2>Item new.ejs</h2>
      <!-- FORM -->
      <div class="">
        <form >
          <div class="form-group">
            <label for="item-title">Title</label>
            <input type="text" class="form-control" id="item" placeholder="Enter Item">
          </div>
          <div class="form-group">
            <label for="item-description">Description</label>
            <textarea type="text" class="form-control" id="item-description" placeholder="Description"></textarea>
          </div>
          <div class="form-group">
            <label for="category-item">Choose a Category</label>
            <select class="form-control" id="category-item">
              <option>Event</option>
              <option>Travel</option>
              <option>Decription</option>
              <option>Other</option>
            </select>
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
        </form>
      </div>
      <!-- FORM END -->
    </div>
    <!-- CONTAINER END -->
  </body>
</html>
